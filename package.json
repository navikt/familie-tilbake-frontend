  "author": "NAV - teamfamilie",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node node_dist/backend/server.js",
    "start:mock": "tsc && NODE_ENV=development node node_dist/dev-server/mock-server.js",
    "start:dev": "tsc && NODE_ENV=development node node_dist/backend/server.js",
    "build": "yarn build:prod",
    "build:dev": "tsc & NODE_ENV=development webpack --config ./build_n_deploy/webpack/webpack.dev.js",
    "build:prod": "tsc & NODE_ENV=production webpack --config ./build_n_deploy/webpack/webpack.prod.js",
    "snyk-protect": "snyk protect",
    "test": "jest",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "yarn run snyk-protect && husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/frontend/tsconfig.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/src/mockFile.js",
      "nav-(.*)-style": "<rootDir>/src/mockFile.js",
      "nav-frontend-core": "<rootDir>/src/mockFile.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@navikt)/)"
    ],
    "transform": {
      "\\.[jt]s?$": "babel-jest",
      "\\.[jt]sx?$": "babel-jest"
    }
  },
