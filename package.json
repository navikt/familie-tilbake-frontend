{
  "name": "familie-tilbake-frontend",
  "version": "0.0.1",
  "main": "node_dist/server.js",
  "author": "NAV - teamfamilie",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node node_dist/backend/server.js",
    "start:dev": "tsc && NODE_ENV=development node node_dist/backend/server.js",
    "build": "yarn build:prod",
    "build:dev": "webpack --config ./build_n_deploy/webpack/webpack.dev.js",
    "build:prod": "tsc & webpack --config ./build_n_deploy/webpack/webpack.prod.js",
    "snyk-protect": "snyk protect",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "yarn run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "dependencies": {
    "@navikt/familie-backend": "^5.0.18",
    "@types/express": "^4.17.8",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.8",
    "@types/request-promise": "^4.1.46",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "react": "^16.13.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "request": "^2.34",
    "request-promise": "^4.2.6",
    "snyk": "^1.399.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/webpack": "4.41.22",
    "@types/webpack-dev-middleware": "3.7.2",
    "@types/webpack-env": "^1.15.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^6.1.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-app": "^6.2.2",
    "express-static-gzip": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^1.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.1",
    "react-hot-loader": "4.13.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "5.1.4"
  },
  "snyk": true
}