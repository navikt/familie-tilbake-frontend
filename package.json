{
  "name": "familie-tilbake-frontend",
  "version": "0.0.1",
  "main": "node_dist/server.js",
  "author": "Nav - tilbake",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --loader ts-node/esm --es-module-specifier-resolution=node node_dist/backend/server.js",
    "start:dev": "tsc && NODE_ENV=development node --loader ts-node/esm --es-module-specifier-resolution=node node_dist/backend/server.js",
    "build": "pnpm lint && pnpm build:prod",
    "build:client": "vite build --config src/frontend/vite.config.js",
    "build:server": "tsc --project tsconfig.json",
    "build:prod": "pnpm build:server && pnpm build:client",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky",
    "generate-types": "openapi-ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx},!src/frontend/generated/**": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "2.2.4",
    "@hey-api/openapi-ts": "0.92.4",
    "@hookform/resolvers": "5.2.2",
    "@navikt/aksel-icons": "8.5.2",
    "@navikt/ds-css": "8.5.2",
    "@navikt/ds-react": "8.5.2",
    "@navikt/ds-tailwind": "8.5.2",
    "@sentry/browser": "10.39.0",
    "@sentry/core": "10.39.0",
    "@tailwindcss/vite": "4.2.0",
    "@tanstack/react-query": "5.90.21",
    "axios": "1.13.5",
    "classnames": "2.5.1",
    "compression": "1.8.1",
    "connect-redis": "9.0.0",
    "constate": "3.3.3",
    "cookie-parser": "1.4.7",
    "countries-list": "3.2.2",
    "date-fns": "4.1.0",
    "deep-equal": "2.2.3",
    "dotenv": "17.3.1",
    "express": "5.2.1",
    "express-session": "1.19.0",
    "express-static-gzip": "3.0.0",
    "http-proxy-middleware": "3.0.5",
    "prom-client": "15.1.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-router": "7.13.0",
    "redis": "5.11.0",
    "tailwindcss": "4.2.0",
    "winston": "3.19.0",
    "zod": "4.3.6",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@axe-core/react": "4.11.1",
    "@eslint/js": "9.39.3",
    "@peculiar/webcrypto": "1.5.0",
    "@sentry/vite-plugin": "4.9.1",
    "@tailwindcss/typography": "0.5.19",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.10",
    "@types/deep-equal": "1.0.4",
    "@types/express": "5.0.6",
    "@types/express-session": "1.18.2",
    "@types/node": "25.3.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "28.1.0",
    "lint-staged": "16.2.7",
    "lodash": "4.17.23",
    "lodash.throttle": "4.1.1",
    "prettier": "3.8.1",
    "rc-tooltip": "6.4.0",
    "react-refresh": "0.18.0",
    "react-select": "5.10.2",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vite": "7.3.1",
