{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "rootDir": ".",
        "baseUrl": ".",
        "module": "esnext",
        "allowSyntheticDefaultImports": true,
        "typeRoots": ["../../node_modules/@types", "../../node_modules", "./@types"],
        "types": ["@testing-library/jest-dom", "vitest/globals", "node"],
        "paths": {
            "@api/*": ["api/*"],
            "@context/*": ["context/*"],
            "@generated/*": ["generated/*"],
            "@generated-new/*": ["generated-new/*"],
            "@hooks/*": ["hooks/*"],
            "@images/*": ["images/*"],
            "@kodeverk/*": ["kodeverk/*"],
            "@komponenter/*": ["komponenter/*"],
            "@pages/*": ["pages/*"],
            "@stores/*": ["stores/*"],
            "@testdata/*": ["testdata/*"],
            "@testutils/*": ["testutils/*"],
            "@typer/*": ["typer/*"],
            "@utils/*": ["utils/*"]
        }
    },
    "exclude": [
        "../../node_modules"
    ],
    "lib": [
        "es2015",
        "esnext"
    ]
}
